
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>@Session["Name"]</title>
    <meta name="viewport" content="width=device-width" />
    <script type="text/javascript" src="~/Scripts/jquery.js"></script>
    <link rel="stylesheet" type="text/css" href="~/Content/CSS/bootstrap.css" />
    <script type="text/javascript" src="~/Scripts/bootstrap.js"></script>
    <script type="text/javascript" src="~/Scripts/parallax.min.js"></script>
    <script type="text/javascript" src="~/Scripts/Main.js"></script>
    <link rel="stylesheet" type="text/css" href="~/Content/CSS/Main.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/CSS/style.css" />
    <title>Index</title>
</head>
<body>

    <div id="MainCarousel" class="carousel slide" data-interval="10000" data-ride="carousel">
        <!-- Indicators -->
        <ol class="carousel-indicators">
            <li data-target="#MainCarousel" data-slide-to="0" class=""></li>
            <li data-target="#MainCarousel" data-slide-to="1" class=""></li>
            <li data-target="#MainCarousel" data-slide-to="2" class="active"></li>
        </ol>
        <div class="carousel-inner" role="listbox">
            <div class="item">
                <img class="first-slide" src="~/Temp_img/demo.jpeg" alt="First slide">
                <div class="container">
                    <div class="carousel-caption" style="background:rgba(0, 0, 0,0.3)">
                        <h1 class="title-beaty">The @Session["Name"]</h1>
                        <p>Small restaurant for small site.</p>
                        
                    </div>
                </div>
                <div style="position:absolute;width:100%;height:100%;background:#fff;z-index:1">

                </div>
            </div>
            <div class="item">
                <img class="second-slide" src="~/Temp_img/1_slide.jpg" alt="Second slide">
                <div class="container">
                    <div class="carousel-caption" style="background:rgba(0, 0, 0,0.3)">
                        <h1 class="title-beaty">The @Session["Name"]</h1>
                        <p>A lot of different stuff.</p>
                    </div>
                </div>
            </div>
            <div class="item active">
                <img class="third-slide" src="~/Temp_img/2_slide.jpg" alt="Third slide">
                <div class="container">
                    <div class="carousel-caption" style="background:rgba(0, 0, 0,0.3)">
                        <h1 class="title-beaty">The @Session["Name"]</h1>
                        <p>That`s clear enought!</p>
                    </div>
                </div>
            </div>
        </div>
        <a class="left carousel-control" href="#MainCarousel" role="button" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#MainCarousel" role="button" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div> <!-- Carousel -->

    <div class="container">
        <div class="row">
            <div class="col-md-4 ">
                <div class="site-logo" id="site-logo" style="margin-left: 0px;">
                    <a  href="@Url.Action("Index", "Home")" hidefocus="true" style="outline: none;">
                        <i><span>The</span></i><span>@Session["Name"]</span>
                    </a>
                </div>
            </div>
            <div class="col-md-2  menu-item">
                <div>
                    <a  href="@Url.Action("index","Menu")">
