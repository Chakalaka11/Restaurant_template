@model string[]
@{
    ViewBag.Title = "Contacts";
}

<div class="container">
    <div class="row">
        <div class="parallax-window" data-parallax="scroll" style="height:1000px;padding-top:100px">
            <div class="col-md-10 col-md-offset-1" style="background:#fff;padding:30px">
                <div class="col-md-6">
                    <h2 class="title-beaty">Where are we?</h2>
                    <h2 class="text-beaty">@Model[0]</h2>
                    <h2 class="title-beaty">When we work?</h2>
                    <h2 class="text-beaty">@Model[2]</h2>
                    <h2 class="title-beaty">Here we are</h2>
                </div>
                <div class="col-md-6">
                    <h2 class="title-beaty">Email</h2>
                    <h2 class="text-beaty">@Model[3].Split('/')[0]</h2>
                    <h2 class="title-beaty">Telephone</h2>
                    <h2 class="text-beaty">@Model[3].Split('/')[1]</h2>
                </div>
                <div style="width:100%;height:500px" id="map"></div>
            </div>
        </div>
    </div>
</div>

<script>
    $(document).ready(function () {
        $('.parallax-window').parallax({ imageSrc: '../../Temp_img/Contact_p.jpeg' });
    });
      function initMap() {
        var mapDiv = document.getElementById('map');
        var map = new google.maps.Map(mapDiv, {
            center: {lat: @Model[1].Split('/')[0], lng: @Model[1].Split('/')[1]},
          zoom: 8
        });
      }
</script>
<script src="https://maps.googleapis.com/maps/api/js?callback=initMap"
        async defer></script>